@helper Tags(System.Web.Mvc.HtmlHelper htmlHelper,
    IEnumerable<MyTinyBlog.Web.MyTinyBlog.Web.ViewModels.Blog.TagViewModel> tags)
{
    if (tags != null)
    {
        foreach (var tag in tags)
        {
            <div class="tags-div">
                @MyTinyBlog.Web.Extensions.ActionLinkExtensions.TagLink(htmlHelper, tag)
            </div>
        }
    }
}
